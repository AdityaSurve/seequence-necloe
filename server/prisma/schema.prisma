generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model myProjects {
  id            Int      @id @default(autoincrement())
  author        String
  width         String
  height        String
  url           String
  download_url  String
  createdAt     DateTime @default(now())
  updatedAt     DateTime @updatedAt
}

model Feedback {
  id            Int      @id @default(autoincrement())
  fullName     String
  email        String
  comment     String
  createdAt     DateTime @default(now())
}

